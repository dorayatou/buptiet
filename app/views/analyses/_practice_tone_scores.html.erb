<div id="content">
<center>
<table class="datalist">

	<th scope="col">第一个拼音</th>
	<th scope="col">第二个拼音</th>
	
	<th scope="col">答题结果</th>
	<th scope="col">答题用时</th>
	<th scope="col">遮盖字符</th>
	<th scope="col">上传时间</th>
	

	<% @practice_datas.each do |data| %>
		<tr class="<%= cycle('list_line_even','list_line_odd') %>">
			
			<td><%= data.first_tone_char %></td>
			<td><%= data.second_tone_char %></td>
			<td><%= data.correct %></td>
			
			
			<td><%= data.consume_time %></td>
			<td><%= data.is_covered %></td>
			<td><%= data.created_at %></td>
			
		</tr>
	<% end %>
</table>
</center>
</div>

